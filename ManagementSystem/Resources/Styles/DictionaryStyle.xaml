    <Style x:Key="LoginChk" TargetType="{x:Type CheckBox}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type CheckBox}">
                    <StackPanel Orientation="Horizontal" Background="#00000000">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CheckStates">
                                <VisualState x:Name="Checked"/>
                                <VisualState x:Name="Unchecked"/>
                                <VisualState x:Name="Indeterminate"/>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Grid HorizontalAlignment="Left" Height="18" Width="18" VerticalAlignment="Center">
                            <Border>
                                <Border.Background>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="#FF55ADD3" Offset="0"/>
                                        <GradientStop Color="#FF63C9F9" Offset="0.168"/>
                                    </LinearGradientBrush>
                                </Border.Background>
                            </Border>
                            <Path Data="M12.91325,2.9310598 L5.2629312,9.5807691 8.3385868,9.5896577 5.3366449,15.516035 13.271756,8.7039601 10.327586,8.776923 z" HorizontalAlignment="Center" Height="14.351" Stretch="Fill" VerticalAlignment="Center" Width="9.292" StrokeThickness="0" Fill="White">
                                <Path.Effect>
                                    <DropShadowEffect BlurRadius="1" ShadowDepth="1" Color="#FF48A6E4"/>
                                </Path.Effect>
                            </Path>
                            <Border x:Name="border" BorderBrush="#FFD4D4D4" BorderThickness="1">
                                <Border.Background>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="#FFF6F6F6" Offset="0"/>
                                        <GradientStop Color="White" Offset="1"/>
                                    </LinearGradientBrush>
                                </Border.Background>
                            </Border>
                        </Grid>
                        <ContentPresenter Margin="10,0,0,0" RecognizesAccessKey="True" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="Center" />
                    </StackPanel>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsChecked" Value="true">
                            <Setter Property="Opacity" TargetName="border" Value="0"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
